{
  "openapi": "3.0.2",
  "info": {
    "title": "Spring boot template",
    "version": "1.0"
  },
  "paths": {
    "/api/v1/healthcheck": {
      "get": {
        "tags": [
          "healthcheck"
        ],
        "description": "Healthcheck endpoint",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/v1/provider": {
      "post": {
        "tags": [
          "provider"
        ],
        "description": "Insert provider",
        "parameters": [
          {
            "in": "header",
            "name": "X-ACCOUNT-ID",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "description": "Provider to be added",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "contentSchema/providerRequestModel.json"
              }
            }
          }
        },
        "responses": {
          "201": {
            "$ref": "responses/201ProviderCreated.json"
          },
          "400": {
            "$ref": "responses/400BadRequest.json"
          },
          "409": {
            "$ref": "responses/409Conflict.json"
          }
        }
      }
    },
    "/api/v1/provider/{provider-id}/item": {
      "post": {
        "tags": [
          "item"
        ],
        "description" : "Add item",
        "parameters": [
          {
            "in": "header",
            "name": "X-ACCOUNT-ID",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "provider-id",
            "in": "path",
            "description": "Id of provider to add item to",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "requestBody": {
          "description": "Item to be added",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "contentSchema/itemRequestModel.json"
              }
            }
          }
        },
        "responses": {
          "201": {
            "$ref": "responses/201ItemCreated.json"
          },
          "400": {
            "$ref": "responses/400BadRequest.json"
          }
        }
      }
    }
  }
}